# Statistical summary of non-numerical data after converting to integers #


install.packages("splus2R")
library(splus2R)
attach(health)
P.S1 <- ifelse(sex == 1,1,0)
P.SM1 <- ifelse(smoke == 1,1,0) 
P.SM2 <- ifelse(smoke == 2,1,0)
P.V1 <- ifelse(vituse == 1,1,0)
P.V2 <- ifelse(vituse == 2,1,0)

statsumm <- lm(rplasma ~ P.S1 + P.SM1 + P.SM2 + P.V1 + P.V2 + quetlet + cals + fat + fiber + alc + chol + bdiet + rdiet + bplasma, data = health)
summary(statsumm)
